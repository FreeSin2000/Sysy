fun @g(@x: i32) {
%entry:
  @x_3 = alloc i32
  %0 = load @x_3
  %1 = add %0, 1
  store %1, @x_3
  ret
}

fun @f(@i: i32, @j: i32): i32 {
%entry:
  %2 = add @i, @j
  ret %2
}

fun @main(): i32 {
%entry:
  jump %while_entry_0

%while_entry_0:
  br 1, %while_body_1, %while_end_2

%while_body_1:
  jump %while_entry_0

%while_end_2:
  %3 = call @f(1, 2)
  ret %3
}
