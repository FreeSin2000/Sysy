fun @half(@x: i32, @y: i32): i32 {
%entry_half:
  %x = alloc i32
  store @x, %x
  %y = alloc i32
  store @y, %y
  %0 = load %x
  %1 = load %y
  %2 = add %0, %1
  %3 = div %2, 2
  ret %3
}

fun @f() {
%entry_f:
  ret
}

fun @main(): i32 {
%entry_main:
  call @f()
  %4 = call @half(10, 12)
  ret %4
}
